			<answer correct="neutral">Table completion</answer>
		</field>
		<field id="2" group="2" type="i:target" mode="0">
			<answer correct="neutral">Short answer questions</answer>
		</field>
		<field id="3" group="3" type="i:target" mode="0">
			<answer correct="neutral">Matching</answer>
		</field>
		<field id="4" group="4" type="i:target" mode="0">
			<answer correct="neutral">Labelling a diagram</answer>
		</field>

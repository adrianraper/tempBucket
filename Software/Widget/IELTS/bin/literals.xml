		<group name="BandScoreCalculator">	
			<lit name="applicationName">Listening and Reading{newline}Score Calculator</lit>
			<lit name="shortName">Listening and Reading Scores</lit>
			<lit name="applicationVersion">Version {version}</lit>
			
			<lit name="listeningQ">How many questions do you think you will get right?</lit>
			<lit name="readingQ">and how many in the reading test?</lit>
			<lit name="preListening">Listening:</lit>
			<lit name="preReading">Reading:</lit>
			<lit name="examQ">for which exam?</lit>
			<lit name="examAC">Academic</lit>
			<lit name="examGT">General Training</lit>
			<lit name="numQuestions">of {questions}</lit>
			<lit name="btnCalculate">Calculate</lit>
			
			<lit name="bandScoreDescriptor">
				<![CDATA[<font family='Helvetica' size='12'>Your scores<br/><b>Listening {bandScoreListening}</b><br/><b>Reading {bandScoreReading}</b></font>]]>
			</lit>
			<lit name="disclaimer">These scores are estimates only.</lit>
			<lit name="disclaimerFull">
				<![CDATA[<textformat leading="-2"><font family='Helvetica' size='10' >Please remember that we calculate these scores based on your own assessment of your strengths and weaknesses in different parts of the IELTS test. You are not guaranteed to get these score results in the actual test.</font></textformat>]]>
			</lit>
			<lit name="statusRestart"></lit>
			<lit name="statusNeedNumber">Type a number.</lit>
			<lit name="statusNeedExamType">Choose an exam.</lit>
		</group>
		<group name="PredictYourBandScore">	
			<lit name="applicationName">Overall Score Calculator</lit>
			<lit name="shortName">Overall Scores</lit>
			<lit name="applicationVersion">Version {version}</lit>
			
			<lit name="intro">What score do you think you will get in each test?</lit>
			<lit name="preListening">Listening:</lit>
			<lit name="preReading">Reading:</lit>
			<lit name="preWriting">Writing:</lit>
			<lit name="preSpeaking">Speaking:</lit>
			<lit name="btnCalculate">Calculate</lit>

			<lit name="bandScoreDescriptor">
				<![CDATA[<font family='Helvetica' size='12'>Your score<br/><b>{bandScore}</b></font>]]>
			</lit>
			<lit name="disclaimer">This score is an estimate only.</lit>
			<lit name="disclaimerFull">
				<![CDATA[<textformat leading="-2"><font family='Helvetica' size='10' >Please remember that we calculate these scores based on your own assessment of your strengths and weaknesses in different parts of the IELTS test. You are not guaranteed to get these score results in the actual test.</font></textformat>]]>
			</lit>
			<lit name="statusRestart"></lit>
		</group>
		<group name="WhereToStudy">	
			<lit name="applicationName">Where do you want to study in the USA?</lit>
			<lit name="shortName">Where to study</lit>
			<lit name="applicationVersion">Version {version}</lit>
			
			<lit name="intro">Type the school name or city where you want to study:</lit>
			<lit name="noResults">No matches were found in the database, please try again.</lit>
			<lit name="btnCalculate">Search</lit>

			<lit name="whatToDo">Double-click below for details</lit>
			<lit name="whatToDoFull">Double-click a name to go to a detailed page about that institution.</lit>
			<lit name="disclaimer">These results are for guidance.</lit>
			<lit name="disclaimerFull">
				<![CDATA[<textformat leading="-2"><font family='Helvetica' size='10' >Check with the faculty you wish to apply to for the exact requirements for your programme of study. Other institutions that are not listed may also accept IELTS. Double-click results for full details.</font></textformat>]]>
			</lit>
			<lit name="statusRestart"></lit>
		</group>
	</language>

<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" xmlns:candidates="skins.ieltsair.assets.candidates.*">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	
	<fx:Declarations>
		<s:DropShadowFilter id="dropShadowFilter" angle="45" strength="0.5" />
	</fx:Declarations>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="down" />
		<s:State name="over" />
		<s:State name="up" />
	</s:states>
	
	<s:HGroup gap="0">
		<s:Label id="labelDisplay" paddingTop="48" />
		<candidates:videolistexpandbutton filters="{[dropShadowFilter]}" />
	</s:HGroup>
	
</s:Skin>

			fontWeight: normal;
			fontAntiAliasType: advanced;
			fontSharpness: 400;
			fontThickness: -200;
			embedAsCFF: true;
		}
		
		@font-face {
			src: url("skins/ielts/assets/HelveticaNeueLTStd-Bd.otf");
			fontFamily: HelveticaNeueLtStdCFF;
			fontWeight: bold;
			fontAntiAliasType: advanced;
			fontSharpness: 400;

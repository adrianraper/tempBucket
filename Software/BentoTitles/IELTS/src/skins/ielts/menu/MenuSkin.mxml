<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx"
		xmlns:controls="com.clarityenglish.bento.view.*"
		width="100%">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("com.clarityenglish.ielts.view.menu.MenuView")]
	</fx:Metadata>
	
	<s:layout>
		<s:VerticalLayout />
	</s:layout>
	
	<s:HGroup width="100%">
		<s:Label text="Road to IELTS" />
		<s:Spacer width="100%" />
		<s:Button label="Logout" />
		<s:Button label="Contact us" />
	</s:HGroup>
	
	<s:HGroup width="100%">
		<s:VGroup>
			<s:Button label="Reading" />
			<s:Button label="Writing" />
			<s:Button label="Speaking" />
			<s:Button label="Listening" />
		</s:VGroup>
		
		<controls:DynamicView width="100%" height="100%" />
		
	</s:HGroup>
</s:Skin>
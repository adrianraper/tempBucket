<!--- The default skin class for the Spark Button component.  

       @see spark.components.Button
        
      @langversion 3.0
      @playerversion Flash 10
      @playerversion AIR 1.5
      @productversion Flex 4
-->
<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
				   xmlns:s="library://ns.adobe.com/flex/spark" 
				   xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
				   minWidth="35" minHeight="35" 
				   alpha.disabled="0.5">
	
	<fx:Metadata>
		<![CDATA[ 
		/** 
		* @copy spark.skins.spark.ApplicationSkin#hostComponent
		*/
		[HostComponent("spark.components.Button")]
		]]>
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="up" />
		<s:State name="over" stateGroups="overStates" />
		<s:State name="overAndSelected" stateGroups="overStates, selectedStates" />
		<s:State name="down" stateGroups="downStates" />
		<s:State name="disabled" stateGroups="disabledStates" />
	</s:states>
		
	<s:Group left="0" right="9" top="0" bottom="-1" horizontalCenter="0" >
		
		<!-- layer 1: shadow -->
		<!--- @private -->

		
		<!-- layer 2: fill -->
		<!--- @private -->
		<s:Rect id="fill" left="0" right="1" top="2" bottom="2" width="35" height="35" topLeftRadiusY="0" topLeftRadiusX="0" topRightRadiusX="6" topRightRadiusY="6" bottomRightRadiusX="6" bottomRightRadiusY="6" >
			<s:fill>
				<s:LinearGradient rotation="0">
					<s:GradientEntry color="0x9C8161" ratio="0.2" 
									 color.overStates="0xAF9475"  
									 color.downStates="0x877057" />
					<s:GradientEntry color="0xC7B299" 
									 color.over="0xD6C4B0" 
									 color.downStates="0xAA9987"/>
				</s:LinearGradient>
			</s:fill>
		</s:Rect>
		
	</s:Group>
	
	<!-- layer 8: text -->
	<!---  @copy spark.components.supportClasses.ButtonBase#labelDisplay -->
	<s:Group left="8">
		<s:Group top="8" left="0" >
			<s:SWFLoader source="@Embed(source='/skins/ielts/assets/assets.swf', symbol='PrintButton')" left="3" />
		</s:Group>
	</s:Group>
	
</s:SparkButtonSkin>

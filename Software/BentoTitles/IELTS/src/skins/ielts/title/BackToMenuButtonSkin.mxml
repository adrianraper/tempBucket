<!--- The default skin class for the Spark Button component.  

       @see spark.components.Button
        
      @langversion 3.0
      @playerversion Flash 10
      @playerversion AIR 1.5
      @productversion Flex 4
-->
<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
				   xmlns:s="library://ns.adobe.com/flex/spark" 
				   xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
				   minWidth="94" minHeight="20" 
				   alpha.disabled="0.5">
	
	<fx:Metadata>
		<![CDATA[ 
		/** 
		* @copy spark.skins.spark.ApplicationSkin#hostComponent
		*/
		[HostComponent("spark.components.Button")]
		]]>
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="up" />
		<s:State name="over" stateGroups="overStates" />
		<s:State name="overAndSelected" stateGroups="overStates, selectedStates" />
		<s:State name="down" stateGroups="downStates" />
		<s:State name="disabled" stateGroups="disabledStates" />
	</s:states>
	

	<s:Group left="-1" right="0" top="0" bottom="-1" horizontalCenter="0" >
			
		<!-- layer 2: fill -->
		<!--- @private -->
		<s:Rect id="fill" left="2" right="1" top="2" bottom="2" width.over="98" width.down="98" 
				>
			<s:fill>
				<s:LinearGradient rotation="90">
					<s:GradientEntry color="0xFFFFFF" 
									 color.overStates="0xFFFFFF"  
									 color.downStates="0xFFFFFF" />
					<s:GradientEntry color="0xFFFFFF" 
									 color.over="0xFFFFFF" 
									 color.downStates="0xFFFFFF"/>
				</s:LinearGradient>
			</s:fill>
		</s:Rect>
		
	</s:Group>
	
	<!-- layer 8: text -->
	<!---  @copy spark.components.supportClasses.ButtonBase#labelDisplay -->
	<s:Group left="20">
		
		<s:Group x="7" y="8" top="0" left="0" >
			<s:SWFLoader source="@Embed(source='/skins/ielts/assets/assets.swf', symbol='BackToMenuButton')" alpha.over="0.6" />
		</s:Group>
		
		<s:Label id="labelDisplay" 
				 color="0x303030"
				 color.over="0x6E6E6E"		 
				 paddingRight="0" paddingLeft="20"
				 textAlign="center"
				 verticalAlign="middle"
				 maxDisplayedLines="1"
				 horizontalCenter="0" verticalCenter="1"
				 left="0" right="22" top="0" bottom="0">
		</s:Label>    
	</s:Group>
</s:SparkButtonSkin>
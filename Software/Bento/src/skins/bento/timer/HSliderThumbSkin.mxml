<?xml version="1.0"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" xmlns:assets="skins.assets.*" useHandCursor="true" buttonMode="true">
    <!-- states -->
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>

    <s:Group width="30" height="100%" verticalCenter="0" useHandCursor="true" buttonMode="true">
        <s:Rect width="100%" height="100%">
            <s:fill>
                <s:SolidColor color="#FFFFFF" alpha="0" />
            </s:fill>
        </s:Rect>

        <assets:arrowthumb visible="{!parentDocument.hostComponent.isTimeFixed}" horizontalCenter="0" verticalCenter="0" />
    </s:Group>

</s:Skin>

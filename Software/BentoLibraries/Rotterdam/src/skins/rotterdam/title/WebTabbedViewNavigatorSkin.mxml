<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		alpha.disabled="0.5"
		creationComplete="onCreationComplete(event)">
	<fx:Metadata>
		[HostComponent("spark.components.TabbedViewNavigator")]
	</fx:Metadata>
	<fx:Script>
		<![CDATA[
			import mx.events.FlexEvent;
			
			protected function onCreationComplete(event:FlexEvent):void { }
		]]>
	</fx:Script>

	<s:states>
		<s:State name="normal" />
		<s:State name="disabled" />
		<!-- Even though this is a desktop skin we need these states to be declared so we can still use mobile components -->
		<s:State name="landscape" />
		<s:State name="portrait" />
	</s:states>

	<s:Group id="contentGroup" left="0" right="0" top="45" bottom="0" minWidth="0" minHeight="0">
		<s:layout>
			<s:BasicLayout />
		</s:layout>
	</s:Group>

	<s:ButtonBar id="tabBar" left="20" top="7" height="38" useHandCursor="true" buttonMode="true"  />
</s:Skin>

<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark" 
    	xmlns:fb="http://ns.adobe.com/flashbuilder/2009">
	<fx:Metadata>
		[HostComponent("spark.components.ViewNavigator")]
	</fx:Metadata>

    <s:states>
        <s:State name="normal" />
        <s:State name="disabled" />
		<!-- Even though this is a desktop skin we need these states to be declared so we can still use mobile components -->
        <s:State name="landscape" />
        <s:State name="landscapeAndOverlay" />
        <s:State name="portrait" />
        <s:State name="portraitAndOverlay" />
    </s:states>
    
    <s:Group id="contentGroup" left="0" right="0" top="0" bottom="0" minWidth="0" minHeight="0">
        <s:layout>
            <s:BasicLayout/>
        </s:layout>
    </s:Group>

</s:Skin>
